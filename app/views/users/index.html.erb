<h1>User Lists</h1>

<table border="1" style="width: 20%">
  <thead>
    <tr>
      <th>Email</th>
      <th>Admin?</th>
      <th>Edit</th>
    </tr>
  </thead>
  <tbody>
<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.admin %></td>
    <td><%= link_to "Edit", edit_admin_user_path(current_user, user.id) %></td>
  </tr>
<% end %>
  </tbody>
</table>
